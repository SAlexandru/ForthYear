<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="InsiderViewer.browsableEntity"
            objectClass="org.eclipse.jdt.core.IJavaElement">
         <action
               class="lrg.insiderviewer.BrowseEntityAction"
               id="InsiderViewer.browseInInsider"
               label="Browse In Insider View"
               menubarPath="addition">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="lrg.insiderviewer.InsiderView"
            id="InsiderViewer.Insider"
            name="Insider"
            restorable="true">
      </view>
   </extension>

</fragment>
