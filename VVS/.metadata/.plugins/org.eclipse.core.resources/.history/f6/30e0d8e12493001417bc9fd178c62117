		String contentLength = "Content-Length: 0\r\n";
		String date = "Date: " + new Timestamp(new Date().getTime()).toString() + "\r\n";
		
	}
	
	public static void send501(OutputStream out) {
		
	}
	
	public static void send400(OutputStream out) {
		
