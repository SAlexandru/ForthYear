		String httpCode_ = "HTTP/1.1 200 OK\r\n";
		String contentLength_ = "Content-Length: 0\r\n";
		String date_ = "Date: " + new Timestamp(new Date().getTime()).toString() + "\r\n";
		
	}
	
	public static void send501(OutputStream out) {
		
	}
	
	public static void send400(OutputStream out) {
		
